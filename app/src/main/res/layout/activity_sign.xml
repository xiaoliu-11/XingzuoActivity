                    />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="天蝎座"
                    android:textSize="12sp"
                    android:textColor="@color/colorAccent"
                    android:layout_marginLeft="50dp"
                    />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="白羊座"
                    android:textSize="12sp"
